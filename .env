# Prototype Appearance Configuration
# Copy this file to .env and customize as needed

# Use generic "AI Platform" text instead of branded logo
# GENERIC_LOGO=true

# Hide the orange "UXD PROTOTYPE" banner
# PROTOTYPE_BAR=false

# Set the default fidelity mode: "high" or "low"
# This determines whether the app starts in high or low fidelity mode
DEFAULT_FIDELITY=high

# GENERIC_LOGO - Set to 'true' to use generic "AI Platform" text instead of branded logo
# Default: false (shows branded logo)
GENERIC_LOGO=false

# PROTOTYPE_BAR - Set to 'false' to hide the orange "UXD PROTOTYPE" banner at the top
# Default: true (shows the banner)
PROTOTYPE_BAR=true

